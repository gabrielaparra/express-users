<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css">
  </head>
  <body>
    <header>
      <h1> Express Users </h1>
      <nav>
        <a href="/"> Home </a>
        <br>
        <% if (typeof currentUser === 'object') { %>
          <a href="/special"> Click this, only for you. </a>
          <br>
          <a href="/my-rooms"> View your Rooms </a>
          <br>
          <a href="/rooms/new"> Add a New Room </a>
          <br>
          <a href="/logout"> Log Out </a>
          <br>
        <% } else { %>
          <a href="/login"> Log in </a>
          <br>
          <a href="/signup"> Sign Up </a>
          <br>
        <% } %>
      </nav>
    </header>

    <%- body %>

  </body>
</html>
